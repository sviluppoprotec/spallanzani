				<xs:element name="CLASSE_ELEMENTO" type="xs:string" />
				<xs:element name="PIANO" type="xs:string" />
				<xs:element name="ORDINE_PIANO" type="xs:integer" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TblPassi">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ISTRUZIONE" type="xs:string" />
				<xs:element name="PASSO" type="xs:integer" />
				<xs:element name="ID_PMP" type="xs:integer" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>